  <header class="masthead">
    <div class="header-content">
      <div class="header-content-inner">
        <h1 style="color:black">Web Developer</h1>
        <hr>
        <p>Jay Lockridge</p>
        <a class="btn btn-primary btn-xl js-scroll-trigger" href="#about">Find Out More</a>
      </div>
    </div>
  </header>

  <section class="bg-primary" id="about">
    <div class="container">
      <div class="row">
        <div class="col-lg-8 text-left">
          <h2 class="section-heading text-white">I'm Pretty Fun</h2>
          <br>
          <p class="text-faded">My name is Jay and I am currently a rails developer for The difference engine. I lik e technology and tacos.My special skills include making frozen pizzas to perfection, fighting dragons, and not knowing the name of a song I have heard 1,000 times.
          </p>
          <a class="btn btn-default btn-xl js-scroll-trigger" href="#services">Keep going!</a>
        </div>

        <div class="col-lg-4">

          <%=image_tag 'Jay_Lockridge.jpg', class:"img-fluid rounded-circle float-right"%>
        </div>
      </div>
    </div>
  </section>

  <section id="services">
    <div class="container">
      <div class="row">
        <div class="col-lg-12 text-center">
          <h2 class="section-heading">Skills</h2>
          <hr class="primary">
        </div>
      </div>
    </div>
    <div class="container">
      <div class="row justify-content-between">
        <div class="col-md-5">
          <h4 class="text-center">Ruby on Rails
          </h4>
          <div class="progress">
            <div class="progress-bar progress-bar-striped progress-bar-animated" role="progressbar" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100" style="width:90%; height:50px">
              <h3 class="text-center">90%</h3>
            </div>

          </div>

          <br>
          <h4 class="text-center">JavaScript
          </h4>
          <div class="progress">
            <div class="progress-bar progress-bar-striped progress-bar-animated" role="progressbar" aria-valuenow="40" aria-valuemin="0" aria-valuemax="100" style="width:70%; height:50px">
              <h3>70%</h3>
            </div>
          </div>
          <br>
          <h4 class="text-center">React
          </h4>
          <div class="progress">
            <div class="progress-bar progress-bar-striped progress-bar-animated" role="progressbar" aria-valuenow="40" aria-valuemin="0" aria-valuemax="100" style="height:4em;width:40%">
              <h3>40%</h3>
            </div>
          </div>
        </div>

        <div class="col-md-5">
          <h4 class="text-center">Java
          </h4>
          <div class="progress">
            <div class="progress-bar progress-bar-striped progress-bar-animated" role="progressbar" aria-valuenow="40" aria-valuemin="0" aria-valuemax="100" style="width:30%; height:50px">
              <h3>30%</h3>
            </div>
          </div>
          <br>
          <h4 class="text-center">GitHub
          </h4>
          <div class="progress">
            <div class="progress-bar progress-bar-striped progress-bar-animated " role="progressbar" aria-valuenow="40" aria-valuemin="0" aria-valuemax="100" style="width:80%; height:50px">
              <h3>80%</h3>
            </div>
          </div>
          <br>
          <h4 class="text-center">Communication
          </h4>
          <div class="progress">
            <div class="progress-bar progress-bar-striped progress-bar-animated" role="progressbar" aria-valuenow="40" aria-valuemin="0" aria-valuemax="100" style="width:85%; height:50px">
              <h3 >85%</h3>
            </div>
          </div>
        </div>
      </div>
    </div>
